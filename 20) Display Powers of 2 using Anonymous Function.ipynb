   "source": [
    "# 20) Display Powers of 2 using Anonymous Function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "0906b94c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the number of terms:10\n",
      "The 2**0 is:1\n",
      "The 2**1 is:2\n",
      "The 2**2 is:4\n",
      "The 2**3 is:8\n",
      "The 2**4 is:16\n",
      "The 2**5 is:32\n",
      "The 2**6 is:64\n",
      "The 2**7 is:128\n",
      "The 2**8 is:256\n",
      "The 2**9 is:512\n"
     ]
    }
   ],
   "source": [
    "a = int(input(\"Enter the number of terms:\"))\n",
    "result = list(map(lambda x:2**x,range(0,a)))\n",
    "for i in range(0,a):\n",
    "    print(f\"The 2**{i} is:{result[i]}\")"
   ]
